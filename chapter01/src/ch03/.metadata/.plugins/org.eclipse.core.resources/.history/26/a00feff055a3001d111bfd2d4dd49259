package ch01;

public class ForMainTest2 {

	public static void main(String[] args) {

		for (int k = 0; k < 5; k++) {
			for (int i = 0; i < k; i--) {
				System.out.printf(" ");
			} // end of i 띄어쓰기
			
			for (int j = 0; j < 1; j++) {
				System.out.printf("*");
			} // end of j 별
			
			System.out.println();
		} // end of k
		
		
		
	}
}

//     *     5번
//   ***   3번
// ***** 1번