package ch01;

public class WhileMainTest4 {

	// 메인 작업자(메인 쓰레드)
	public static void main(String[] args) {

		for (int i = 0; i < 11; i++) {
			System.out.println("i : " + i);
			// 만약 i값이 소수 7이면 멈춰라 !
			// 소수 : 나누었을때 0이되는것
			if (i % 7 == 0) {
				break;
			}
		}

	} // end of main

} // end of class
