package ch10;

public class A {
	String name;
	int height;
	int weight;
	int age;

} // end of A class

// 하나의 자바파일에는 public이 하나만 존재
// 하나의 자바파일에는 여러개의 class 만들 수 있음

// 코드 테스트
	public static void main(String[] args) {

	}

class B {
	String name;
	int height;
	int weight;
	int age;
}
// end of B class