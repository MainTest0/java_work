package ch01;

public class ForMainTest2 {

	public static void main(String[] args) {

		for (int k = 0; k < 5; k++) {
			for (int i = 0; i < 5; i++) {
				System.out.printf(" ");
			}
			for (int j = 0; j < 1; j++) {
				System.out.printf("*");
			}
		}
	}
}

//     *     5번
//   ***   3번
// ***** 1번