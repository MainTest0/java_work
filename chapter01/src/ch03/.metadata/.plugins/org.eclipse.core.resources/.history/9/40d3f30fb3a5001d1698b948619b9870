package ch06;

import java.util.Random;
import java.util.Scanner;

public class ExTest {
	
	static int Rand() {
		Random r = new Random();
		for(int i =1; i<7; i++) {
			int rd = r.nextInt(45)+1;
			System.out.print(rd+" ");
		}
		
		return 0;
	}

	public static void main(String[] args) {

		System.out.println(Rand());
		
		
		
	}

}
