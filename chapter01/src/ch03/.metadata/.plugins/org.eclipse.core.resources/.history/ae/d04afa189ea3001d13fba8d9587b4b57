package ch06;

import java.util.Scanner;

public class ExTest {
	
	Scanner sc = new Scanner(System.in);
	
	static void humanName(String name) {
		
		System.out.println(name);
		//void : return 값이 없을 때
		
	}
	
	static String humanName1(String name) {
		
		return name;
	}

	public static void main(String[] args) {

		humanName("d");
		
		
		String n = humanName1("q");
		System.out.println(n);

	}

}
