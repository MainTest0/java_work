package ch07;

public class Person {

	//this의 이용방법 3가지
	// 1. 자기 자신을 가리킨다.
	private String name;
	private int age;
	private String phone;
	private String gender;

	public Person(String name, int age) {
		// 1번에 대한 사용법 ( 자기 자신을 가리키는 this)
		this.name = name;
		this.age = age;
	}
	// 생성자는 객체가 메모리에 올라갈 때 제일 먼저 실행하는 코드이다.
	 public Person(String name, int age, String phone) {
		 this.name = name;
		 this.age = age;
		 this.phone = phone;
	 }
	
}// end of class
