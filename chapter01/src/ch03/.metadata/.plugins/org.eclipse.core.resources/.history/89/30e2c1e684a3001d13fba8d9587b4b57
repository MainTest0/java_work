package ch06;

import ch05.MainTest1;

public class FunctionMainTest {

	// 함수를 설계하는 코드 작성
	static int addNumber(int n1, int n2) { // 시그니쳐

		// 함수 구현부 (body)

		int result; // 지역변수
		result = n1 + n2;

		return result;
	}

	// 텅빈, 리턴값이 없다
	public static void main(String[] args) {
		// 함수 호출하기

		addNumber();
	}
} // end of class
