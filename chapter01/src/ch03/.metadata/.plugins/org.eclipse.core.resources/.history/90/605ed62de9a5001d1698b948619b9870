package ch04;

// 전사 클래스 설계하기

// 마나가 없어요 마나채우기
// 공격스킬
public class Warrior {

	int HP;
	int MP;
	int power;
	
	//상태
	// 레벨, 파워, 체력, 마나
	public Warrior() {
		
	}
	
	//기능
	// 기본공격(마나x), q스킬(마나), r스킬(궁)
	public void basicAttack() {
		System.out.println("기본공격");
	}
	public void skillQ() {
		System.out.println("올려치기");
	}
	public void skillR() {
		System.out.println("내려꽂기");
		
	}
	
}//end of class
