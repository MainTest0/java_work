package Ex13;

public class MainTest {
	public static void main(String[] args) {
		
		// 우리반 학생 이름들을 배열에 담아주세요
		// 랜덤 클래스를 사용해서
		// 한명을 추출 해주세요
		// 편용림 김효린 배진석 강민정 한범진 손주이 김유주 김미정 김지현 박성희 이지운 정다운 이치승 이서영 전대영 김현우 김민우 이현서
		
		String[] std = {"편용림", "김효린", "배진석", "강민정","한범진", "손주이", "김유주", "김미정", "김지현", "박성희", "이지운", "정다운", "이치승", "이서영", "전대영", "김현우", "김민우", "이현서"};
		
		for(int i = 0; i < std.length; i++) {
			System.out.println(std[i]);
		}
		
	}
}
