package ch14;

public class ManualCar extends Car {
	
	public static void main(String[] args) {
		
		Car aicar = new AiCar();
		aicar.drive();
		
	}

	@Override
	public void drive() {
		System.out.println("사람이 운전을 합니다.");

	}

	@Override
	public void stop() {
		System.out.println("사람이 브레이크를 밟습니다.");

	}

	@Override
	public void wiper() {
		System.out.println("사람이 작동을 시킵니다.");

	}

}
