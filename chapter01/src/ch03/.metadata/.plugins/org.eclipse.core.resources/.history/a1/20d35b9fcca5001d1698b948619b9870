package ch02;

public class Human {

	String name;
	int height;
	int weight;
	boolean isMan;

	// 생성자는 객체가 메모리에 올라갈 때 가장 먼저 수행되는 코드이다.
	public Human() {

	}

	public void showInfo() {
		System.out.println("===============상태창=================");
		System.out.println("키 : " + height);
		System.out.println("몸무게 : " + weight);
		System.out.println("성별 : " + isMan);
		System.out.println("이름 : " + name);
	}

}
