package ch04;

public class Bus {
	
	// 상태
	// 버스번호, 승객 수, 수익금
	
	int busNumber;		//버스 호선
	int passangerCount;		// 승객수
	int money;		// 수익금
	
	// 기능
	// 달린다
	public void run() {
		System.out.println("버스가 출발합니다.");
	}
	
	// 승차 시키다
	public void take() {
		System.out.println("승객을 태웁니다.");
	}
	
	// 하차 시키다
	public void takeOff() {
		System.out.println("승객을 태웁니다.");
	}
	public void showInfo() {
		System.out.println("상태 창");
		System.out.println("지하철 호선 정보 ; " + this.lineNumber());
		System.out.println("지하철 승객 수; " + this.passangerCount());
		System.out.println("지하철 수익금 ; " + this.money());
	}
	
	
}
