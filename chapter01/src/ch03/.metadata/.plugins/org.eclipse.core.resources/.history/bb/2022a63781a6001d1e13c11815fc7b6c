package ch05;

public class MainTest1 {
    //메인 함수
	public static void main(String[] args) {
		
		Student studentKim = new Student("홍길동", 10_000 );
		Bus bus100 = new Bus(100);
		Bus bus200 = new Bus(200);
		// 객체와 객체간의 메세지 전달하기(상호작용 하기)

		studentKim.takeBus(bus200);
		
	}// end of main

}// end of class
