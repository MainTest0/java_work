package ch01;

public class WhileMainTest1 {

	// 메인 작업자
	public static void main(String[] args) {

		// 1부터 10까지 연산하는 코드
		int num = 1;
		int sum = 0;

		while (num <= 1000) {
			System.out.println("현재값 : " + num); // 무한 루프에 조심
			sum += num;
			num++;
		}
		System.out.println("연산에 대한 결과 값은 : " +sum+ "입니다.");

	} // end of main

} // end of class
